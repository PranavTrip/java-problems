package Pepcoding;

import java.util.Scanner;

public class Digitsinnumber {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int num = sc.nextInt();
        int digits = 0;
        while (num != 0) {
            int number = num % 10;
            num = num / 10;
            digits++;
        }
        System.out.println(digits);
    }
}
